<swiper class="swiper" circular 
        autoplay 
        interval="2000" 
        duration="200"
        indicator-dots
        indicator-active-color="#c800f0">
        <block wx:for="{{images}}" wx:key="index">
              <swiper-item class="swiper-item">
                    <image src="{{item}}" mode="widthFix" />
              </swiper-item>
        </block> 
</swiper>